@model List<Sindicato_v1.Models.ViewModels.EditGestionesViewModel>

@{
    ViewBag.Title = "Editar Gestiones";
}
<br>
<br>
<nav class="navbar navbar-default navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="#inicio">SIICE</a>
        </div>
        <div class="collapse navbar-collapse" id="myNavbar">
            <ul class="nav navbar-nav navbar-right">
                <li><a href="~/Agremiado/Agremiados">Agremiados</a></li>
                <li><a href="~/Deducciones/BusquedaDeduccion">Deducciones</a></li>
                <li><a href="">Noticias - Foro</a></li>
                <li><a href="~/Gestiones/Gestiones">Gestiones</a></li>
                <li><a>Hola, @ViewData["Nombre"] @ViewData["Apellido"]</a></li>
            </ul>
        </div>
    </div>
</nav>

<ul class="breadcrumb">
    <li><a href="/Home/Index/">Inicio</a></li>
    <li><a href="/Usuario/Administrador/">Administrativo</a></li>
    <li><a href="/Gestiones/Gestiones/">Gestiones</a></li>
    <li>Control de Gestiones</li>
</ul>

<h2>Control de Gestiones</h2>

<hr class="hr-custom">

<div class="container">
    <div class="table-responsive">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>Código</th>
                    <th>Motivo</th>
                    <th>F Inicio</th>
                    <th>F Fin</th>
                    <th>Nombre</th>
                    <th>Apellido</th>
                    <th>Tipo Gestión</th>
                    <th>F Ausencia</th>
                    <th>Acciones</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var obj_Ges in Model)
                {
                    <tr>
                        <th>@obj_Ges.Id_Ges</th>
                        <td>@obj_Ges.Motivo</td>
                        <td>@obj_Ges.F_Inicio</td>
                        <td>@obj_Ges.F_Fin</td>
                        <td>@obj_Ges.Nombre</td>
                        <td>@obj_Ges.Ap_Paterno</td>
                        <td>@obj_Ges.T_Gestion</td>
                        <td>@obj_Ges.F_Ausencia</td>
                        <td>
                            <a class="btn btn-default" href="/Gestiones/Eliminar_Gestion/@obj_Ges.Id_Ges" style="width:80px; height:35px;">Eliminar</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

